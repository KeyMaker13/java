import java.util.*;

public class stringAgainFour
{
  Scanner console = new Scanner (System.in)
  
    System.out.println("Enter a string, then i will stop asking for more strings/n untill you enter the same string twice");
  
  String first = console.next();
  
  String nextFirst = "";
  String nextSecond = "";
  boolean stop = false;
  
  while(!stop)
  {
    nextFirst = console.next();
    nextSecond = console.next();
    if (nextFirst == nextSecond)
    {
      stop = true;
    }
  }
}
