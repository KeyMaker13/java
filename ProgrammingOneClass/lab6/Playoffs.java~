import java.util.*;

public class Playoffs
{
  Random rand = new Random();
  public static void main (String[] args)
  {
    
    
  
  }
  
  
  public static int singleGame(int percent, Random r = new Random())
  {
    int gameRandom = 1 + r.nextInt(101);
    
    if (gameRandom < percent)
    {
      return 1;
    }
    else
    {
      return 0;
    }
  }
  
  
  
  
  
}